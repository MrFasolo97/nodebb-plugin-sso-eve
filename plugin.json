{
	"id": "nodebb-plugin-sso-eve",
	"name": "NodeBB OAuth SSO for EVE Online",
	"description": "NodeBB Plugin that allows users to login/register via EVE Online SSO.",
	"url": "https://github.com/MrFasolo97/nodebb-plugin-sso-eve",
	"library": "./library.js",
	"hooks": [
		{ "hook": "static:user.delete", "method": "deleteUserData" },
		{ "hook": "filter:user.whitelistFields", "method": "whitelistFields" },
		{ "hook": "filter:auth.init", "method": "getStrategy" }
	]
}
